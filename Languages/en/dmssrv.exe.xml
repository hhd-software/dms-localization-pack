<?xml version="1.0" encoding="utf-8"?>
<Definition xmlns="http://www.hhdsoftware.com/schemas/langset/1.0.xsd">

  <Message key="ServiceFriendlyName" description="DMS Server service friendly name" value="Device Monitoring Studio Monitoring Server"/>

  <Message key="ServiceDescription" description="DMS Server service description" value="Device Monitoring Studio Monitoring Server service provides serial, USB and network devices monitoring capabilities for remote clients. Local computer and domain administrators may use API provided by the service to manage connections and security of the server."/>

  <Message key="LicenseSuccess" description="License installation successful" value="License has been successfully installed."/>
  <Message key="LicenseFailure" description="Error installing license" value="There was an error during license installation. Contact HHD Software."/>

  <Message key="CurrentLicense" description="Current license" value="{unknown?}Unknown License{|unknown}{license type}{?unknown}{trial?}, {days} days left{?trial}{expired?}, expired{?expired}">
    <Param name="unknown" format="?"/>
    <Param name="license type"/>
    <Param name="trial" format="?"/>
    <Param name="expired" format="?"/>
    <Param name="days"/>
  </Message>

  <!-- Generic strings -->
  <Message key="EmergencySessionTermination" description="Emergency session termination error" value="{error text} ({error code})">
    <Param name="error text"/>
    <Param name="error code" format="ub016"/>
  </Message>

  <Message key="Access Denied" description="Access Denied"/>
  <Message key="NoSlots" description="No slots" value="No more connection slots available"/>
  <Message key="SetNameError" description="Session creation error" value="Network error: {error text} ({error code})">
    <Param name="error text"/>
    <Param name="error code" format="ub016"/>
  </Message>

  <Enumeration key="LicenseType" description="License type">
    <Item text="Unknown"/>
    <Item text="Free"/>
    <Item text="Trial"/>
    <Item text="Expired"/>
    <Item text="Registered"/>
  </Enumeration>

</Definition>
